{
  "name": "money-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register start.ts",
    "build": "tsc && tsc-alias",
    "start": "node ./dist/start.js",
    "docker:up": "docker-compose up -d --build",
    "docker:down": "docker-compose down",
    "test:integration": "yarn docker:up && jest --testPathPattern=integration --maxWorkers=1",
    "coverage:integration": "yarn docker:up && jest --testPathPattern=integration --maxWorkers=1 --coverage",
    "test:unit": "jest --testPathIgnorePatterns=./*.integration.spec.ts",
    "coverage:unit": "jest --testPathIgnorePatterns=./*.integration.spec.ts --coverage",
    "ci:test:integration": "jest --testPathPattern=integration",
    "ci:coverage:integration": "jest --testPathPattern=integration --coverage",
    "ci:test:unit": "jest --testPathIgnorePatterns=./*.integration.spec.ts",
    "ci:coverage:unit": "jest --testPathIgnorePatterns=./*.integration.spec.ts --coverage",
    "ci:start": "yarn prisma migrate deploy && yarn build && yarn start",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "4.0.0",
    "@prisma/migrate": "^4.0.0",
